<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">
<bean id="std" class="com.sp.Student">
<property name="sid" value="1"></property>
<property name="sname" value="Moksh"></property>
<property name="degree" value="BTech"></property>
</bean>
<bean id="s1" parent="std">
<property name="address" value="Paldi"></property>
<property name="city" value="Ahmedabad"></property>
<property name="state" value="Gujarat"></property>
</bean>
</beans>
